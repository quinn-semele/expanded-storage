{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${fl_authors}"
    ],
    "contact": {
        "homepage": "${homepage}",
        "issues": "${issue_tracker}",
        "sources": "${sources_url}"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.example.examplemod.ExampleMod"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": ">=${fabric_api_version}",
        "minecraft": "${fl_minecraft_constraint}",
        "java": ">=${java_version}"
    }
}
