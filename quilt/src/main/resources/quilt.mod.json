{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${description}",
      "contributors": {

      },
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/example-mod",
        "issues": "https://github.com/Ghost/example-mod/issues",
        "sources": "https://github.com/Ghost/example-mod"
      },
      "license": "${license}",
      "icon": "${mod_id}.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "main": "com.example.examplemod.ExampleMod"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${quilt_loader_version}"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=${quilt_api_version}"
      },
      {
        "id": "minecraft",
        "versions": "${fl_minecraft_constraint}"
      },
      {
        "id": "java",
        "versions": ">=${java_version}"
      }
    ]
  },
  "mixin": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ]
}
